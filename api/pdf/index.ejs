<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="styles.css">

    <title>Document</title>
</head>

<body>
    <style>
        h3 {
            color: rgb(49, 49, 49);
            margin: 1rem;
        }

        table {
            width: 100%;
            border: 1px solid #ccc;
            border-collapse: collapse;
        }

        thead {
            border: 1px solid #ccc;
            border-collapse: collapse;
            background: #0078ff;
            text-align: left;
            color: #fff;
        }

        th,
        td {
            font-size: 0.8rem;
            padding: 1rem 1rem;
        }

        td {
            font-size: 0.8rem;
        }

        tbody tr:nth-child(even) {
            background: #0257ff17;
        }

        td a {
            margin-left: 10px;
            decoration: none;
        }
    </style>
    <h3>Lista de Alunos</h3>
    <table>
        <thead>
            <tr>
                <th>Código</th>
                <th>Nome</th>
                <th>Data</th>
                <th>Horário</th>
            </tr>
        </thead>

        <tbody>
            <% list.forEach(function(list) { %>

                <tr>
                    <td>
                        <%= list.codigo %>
                    </td>
                    <td>
                        <%= list.name %>
                    </td>
                    <td>
                        <%= list.date %>
                    </td>
                    <td>
                        <%= list.horario %>
                    </td>
                </tr>

                <% }) %>
        </tbody>
    </table>
</body>

</html>